{
    "name": "redmudlib",
    "version": "0.0.10",
    "description": "A MUD Redis-access library written in NodeJS.",
    "main": "mud-lib.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/jyurkiw/RedMUDLib.git"
    },
    "scripts": {
        "test": "./node_modules/.bin/mocha --trace-warnings --reporter spec",
        "testc": "./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha",
        "jsdoc": "./node_modules/.bin/jsdoc ./mud-lib.js ./lib -r --readme ./README.md -d docs",
        "wiki": "./node_modules/.bin/jsdoc2md mud-lib.js lib/*/*.js > api.md"
    },
    "keywords": [
        "mud",
        "game",
        "server"
    ],
    "author": "Jeff Yurkiw",
    "license": "MIT",
    "dependencies": {
        "bluebird": "^3.4.6",
        "node-gameloop": "^0.1.0",
        "redis": "^2.6.2"
    },
    "devDependencies": {
        "chai": "^3.5.0",
        "istanbul": "^0.4.5",
        "jsdoc": "^3.4.2",
        "jsdoc-to-markdown": "^2.0.1",
        "jshint": "^2.9.4",
        "mocha": "^3.1.2"
    }
}